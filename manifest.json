{
  "manifest_version": 3,
  "name": "Privacy Shield",
  "version": "1.0",
  "description": "A privacy-focused extension to block known tracking scripts",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "storage",
    "activeTab"
  ],
  "host_permissions": ["&lt;all_urls&gt;"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}


